#include<iostream>
#include<cmath>
using namespace std;
int main ()
{
    int t;
    cin>>t;
    while(t--)
    {   
        // 2 mang
        int n,m;
        cin>>n>>m;
        int a[n],b[m];
        for(int i=0;i<n;i++)
            cin>>a[i];
        for(int i=0;i<m;i++)
            cin>>b[i];
//        int max=0,min=0; sai neu la so am
        long long max=a[0],min=b[0];
            for(int i=0;i<n;i++)
            {
                    if(a[i]>max)
                     max=a[i];
            }
            for(int i=0;i<m;i++)
            {
                    if(b[i]<min)
                     min=b[i];
            }
            cout<<max*min<<endl;
    }
}
