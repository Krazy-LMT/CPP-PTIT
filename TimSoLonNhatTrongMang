#include<iostream>
#include<cmath>
using namespace std;
int main ()
{
    int t;
    cin>>t;
    while(t--)
    {   
         int n;
         cin>>n; //nhap so luong phan tu
         int a[n];
         for(int i=0;i<n;i++)
         cin>>a[i];           // nhap tung phan tu trong mang
        int max=a[0];
        for(int i=0;i<n;i++) // lay 1 so lam goc so sanh voi cac so khac
        {
            for(int j=0;j<n;j++)
            {
                if(a[i]>max)
                max=a[i];
            }
        }
        cout<<max<<endl;
}
