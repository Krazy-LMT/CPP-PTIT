#include<iostream>
#include<cmath>
using namespace std;
int main ()
{
    int t;
    cin>>t;
    while(t--)
    {   
         int n,k;
         cin>>n>>k; //nhap so luong phan tu
         int a[n];
         for(int i=0;i<n;i++)
         cin>>a[i];           // nhap tung phan tu trong mang
        for(int i=0;i<n;i++) // lay 1 so lam goc so sanh voi cac so khac
        {
            for(int j=i+1;j<n;j++)
            {
                if (a[i] < a[j])
                 {
                    int temp = a[i];
                    a[i] = a[j];
                    a[j] = temp;
                 }
            }

        }
        for(int i=0;i<k;i++)
        {
            cout<<a[i]<<" ";
        }
        cout<<endl;
    }
}
